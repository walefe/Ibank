{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "builder": "swc",
    "typeCheck": true,
    "deleteOutDir": true,
       "assets": [
      {
        "include": "shared/database/prisma/client/**/*",
        "watchAssets": true
      }
    ]
  }
}
